{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "Node",
    "outDir": "dist",
    "baseUrl": "src/",
    "sourceMap": false,
    "strict": false,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false
  },
  "exclude": [
    "node_modules"
  ]
}
